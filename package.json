  "description": "Vuejs Typescript SPA",
  "main": "index.js",
  "scripts": {
    "build": "rimraf dist && cross-env NODE_ENV=production webpack --config build/webpack.config.ts --hide-modules --progress",
    "build:dev": "rimraf dist && cross-env NODE_ENV=development webpack --config build/webpack.config.ts --hide-modules --progress",
